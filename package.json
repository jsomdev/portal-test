{
  "name": "digitalhighway-next-sprayportal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 3002",
    "dev:proxy": "concurrently --kill-others \"local-ssl-proxy --source 3000 --target 3002\" \"cross-env NODE_OPTIONS='--inspect' next dev -p 3002\"",
    "build": "next build",
    "start": "next start -p 3002",
    "start:proxy": "concurrently --kill-others \"local-ssl-proxy --source 3000 --target 3002\" \"npm run start\"",
    "lint": "next lint --dir pages --dir utilities --dir widgets --dir services --dir providers",
    "lint:nowarn": "next lint --dir pages --dir utilities --dir widgets --dir services --dir providers --quiet",
    "lint:fix": "next lint --dir pages --dir utilities --dir components --dir widgets --dir services --dir providers --fix",
    "test": "cross-env  NODE_ENV='test' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },

  "dependencies": {
    "@artsy/fresnel": "^6.1.0",
    "@azure/msal-browser": "^2.18.0",
    "@azure/msal-react": "^1.1.0",
    "@fluentui/react": "^8.87.1",
    "card-validator": "^8.1.1",
    "flat": "^5.0.2",
    "formik": "^2.2.9",
    "gray-matter": "^4.0.3",
    "guid-typescript": "^1.0.9",
    "next": "^12.2.4",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.10",
    "react-json-formatter": "^0.3.2",
    "react-markdown": "^7.0.1",
    "react-query": "^3.33.1",
    "sharp": "^0.30.7",
    "ua-parser-js": "^1.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/flat": "^5.0.2",
    "@types/jest": "^27.0.2",
    "@types/nprogress": "^0.2.0",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "axe-core": "^4.3.3",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-formatjs": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.js": "lint:fix"
  }
}
