{
  "name": "digitalhighway-next-sprayportal",
  "version": "2.0.0-rc.2",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 3002",
    "dev:proxy": "concurrently --kill-others \"local-ssl-proxy --source 3000 --target 3002\" \"cross-env NODE_OPTIONS='--inspect' next dev -p 3002\"",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 3002",
    "start:proxy": "concurrently --kill-others \"local-ssl-proxy --source 3000 --target 3002\" \"npm run start\"",
    "lint": "next lint --dir pages --dir utilities --dir widgets --dir services --dir providers",
    "lint:nowarn": "next lint --dir pages --dir utilities --dir widgets --dir services --dir providers --quiet",
    "lint:fix": "next lint --dir pages --dir utilities --dir components --dir widgets --dir services --dir providers --fix",
    "test": "cross-env  NODE_ENV='test' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "lint-staged",
    "format": "prettier -write -config .prettierrc",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE='true' next build"
  },
  "lint-staged": {
    "*": [
      "eslint -c .eslintrc.js --fix",
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "@artsy/fresnel": "6.1.0",
    "@azure/msal-browser": "2.32.0",
    "@azure/msal-react": "1.5.0",
    "@fluentui/react": "8.103.4",
    "@microsoft/applicationinsights-react-js": "3.4.0",
    "@microsoft/applicationinsights-web": "2.8.9",
    "@next/bundle-analyzer": "13.1.2",
    "@types/react-pdf": "5.7.4",
    "card-validator": "8.1.1",
    "flat": "5.0.2",
    "formik": "2.2.9",
    "gray-matter": "4.0.3",
    "guid-typescript": "1.0.9",
    "next": "12.3.4",
    "next-sitemap": "3.1.45",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-cookie": "4.1.1",
    "react-dom": "17.0.2",
    "react-intl": "5.25.1",
    "react-json-formatter": "0.3.2",
    "react-markdown": "7.1.2",
    "react-pdf": "5.7.2",
    "react-query": "3.39.2",
    "recharts": "2.1.16",
    "sharp": "0.30.7",
    "swiper": "8.4.5",
    "ua-parser-js": "1.0.32",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.1",
    "@trivago/prettier-plugin-sort-imports": "3.4.0",
    "@types/flat": "5.0.2",
    "@types/jest": "27.5.2",
    "@types/nprogress": "0.2.0",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "axe-core": "4.5.2",
    "concurrently": "6.5.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-formatjs": "4.3.7",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "13.0.4",
    "prettier": "2.8.0",
    "typescript": "4.9.3"
  }
}
