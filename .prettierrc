{
  "tabWidth": 2,
  "printWidth": 80,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "trailingComma": "none",
  "semi": true,
  "useTabs": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "importOrder": ["^react(/)?$", "<THIRD_PARTY_MODULES>", "^@", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
